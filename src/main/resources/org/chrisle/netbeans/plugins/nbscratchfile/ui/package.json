        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.2.1"
    },
    "scripts": {
        "build-dev": "npm run test && npm run svg2png && webpack --mode development",
        "build-prod": "npm run test && npm run svg2png && webpack --mode production --optimize-minimize",
        "fast-build-dev": "webpack",
        "svg2png": "svg2png-many -i ./app/icons -o ./.tmp/icons -w 20 -h 20 && node ./build-scripts/gen-sprite.js",
        "sourcemap-explorer": "source-map-explorer ./dist/app.js ./dist/app.js.map",
        "analyze-package": "webpack-bundle-analyzer ./reports/webpack-bundle-analyzer/stats.json",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor",
        "serve": "npm run svg2png && webpack-dev-server -d",
        "test": "mocha -r ts-node/register app/**/*.spec.ts",
        "ver": "webpack -v"
    }
}
